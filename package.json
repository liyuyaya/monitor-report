{
  "name": "monitor-report",
  "version": "0.0.0-test0.0.3",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "description": " tool used to assist the front-end in collecting errors, operations, PV/UV statistics, etc （一个用来帮助前端收集错误、操作、pv / uv 统计等）",
  "keywords": [
    "monitor-report",
    "monitor",
    "report",
    "vue",
    "react"
  ],
  "author": {
    "name": "liyu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liyuyaya/monitor-report"
  },
  "homepage": "https://github.com/liyuyaya/monitor-report",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/monitor.report.esm.min.js"
      }
    },
    "./types": "./index.d.ts",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "yarn clear &&  cross-env NODE_ENV=production rollup -c && yarn build:dev ",
    "build:dev": "cross-env NODE_ENV=dev rollup -c ",
    "dev": "cross-env NODE_ENV=dev  rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clear": "rimraf ./dist"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime-corejs3": "^7.24.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.7",
    "rollup": "3",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
