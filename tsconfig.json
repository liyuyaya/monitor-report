{
  "include": [
    "./src/**/*",
    "src/typing.d.tsd.ts",
    "src/typing.d.tsd.ts",
    "src/index.tsx.ts",
    "src/index.tsx.ts",
    "index.d.ts",
    "index.d.ts",
    "env.d.ts",
    "types/*.d.ts",
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  }
}